{
  "name": "@polywrap/landing-page",
  "version": "0.1.0",
  "homepage": "https://polywrap.io/",
  "scripts": {
    "start": "serve -s build",
    "dev": "react-scripts start",
    "build": "CI=false react-scripts build && echo polywrap.io > build/CNAME",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "reset": "yarn clean && yarn",
    "clean": "npx rimraf ./**/node_modules ./**/build ./**/coverage",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "deploy:ci": "gh-pages -d build -b ci-$(git rev-parse HEAD | cut -c 1-10)"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^5.0.0-beta.0",
    "hex-to-css-filter": "^3.1.2",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-ga": "^3.3.0",
    "react-lottie": "1.2.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-scroll-parallax": "^2.4.0"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-lottie": "1.2.5",
    "@types/react-router-dom": "^5.1.6",
    "babel-plugin-prismjs": "^2.1.0",
    "gh-pages": "^3.1.0",
    "rimraf": "3.0.2",
    "serve": "^11.3.2",
    "typescript": "^4.0.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
